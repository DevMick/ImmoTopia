{
  "name": "@standard-app/api",
  "version": "1.0.0",
  "description": "StandardApplication Template Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:crm": "ts-node prisma/seeds/seed-crm-data.ts",
    "db:seed:comprehensive": "ts-node prisma/seeds/seed-comprehensive-data.ts",
    "db:seed:rbac": "ts-node prisma/seeds/rbac-seed.ts",
    "db:assign:rbac": "ts-node prisma/seeds/assign-rbac-roles.ts",
    "db:fix:appointment": "ts-node scripts/fix-appointment-stage.ts",
    "db:fix:rental-permissions": "ts-node prisma/seeds/fix-rental-permissions.ts",
    "check:missing-lease-data": "ts-node scripts/check-missing-lease-data.ts",
    "check:property-data": "ts-node scripts/check-property-data.ts",
    "db:assign:tenant-admin": "ts-node prisma/seeds/assign-tenant-admin.ts",
    "db:list:tenant-users": "ts-node prisma/seeds/list-tenant-users.ts",
    "db:seed:document-templates": "ts-node prisma/seeds/document-templates-seed.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "standard-app",
    "api",
    "template"
  ],
  "author": "StandardApplication Team",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/crypto-js": "^4.2.2",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.2.0",
    "docxtemplater": "^3.67.6",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pizzip": "^3.2.0",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
